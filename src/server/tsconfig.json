{
    "compilerOptions": {
      "target": "es2020",
      "lib": ["es2020"],
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "../dist-server",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "include": ["./**/*"]
  }